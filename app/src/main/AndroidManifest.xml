<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="group8.tcss450.uw.edu.group8project">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/display"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="group8.tcss450.uw.edu.group8project.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="group8.tcss450.uw.edu.group8project.LoginActivity" />
        <activity android:name="group8.tcss450.uw.edu.group8project.DisplayActivity" />
        <activity android:name="group8.tcss450.uw.edu.group8project.RegisterActivity"></activity>
    </application>

</manifest>